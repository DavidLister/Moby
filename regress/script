#/usr/bin/bash
# contact simple regressions
moby-regress -s=.001 -mi=1000 ../example/contact_simple/contact.xml stationary-box.out
moby-regress -s=.001 -mi=1000 ../example/contact_simple/spinning-box1.xml spinning-box1.out
moby-regress -s=.001 -mi=1000 ../example/contact_simple/spinning-box2.xml spinning-box2.out

# deformable body regressions
moby-regress -s=.001 -mt=10 ../example/bouncing-sphere/sphere.xml bouncing-sphere.out

# slider/crank regressions (RCAB)
moby-regress -s=.001 -mt=10 ../example/slider-crank/rcab/pendulum.xml pendulum-rcab.out
moby-regress -s=.001 -mt=10 ../example/slider-crank/rcab/pendulum2.xml pendulum2-rcab.out
moby-regress -s=.001 -mt=10 ../example/slider-crank/rcab/dpendulum.xml dpendulum-rcab.out
moby-regress -s=.001 -mt=10 ../example/slider-crank/rcab/dpendulum2.xml dpendulum2-rcab.out
moby-regress -s=.001 -mt=10 ../example/slider-crank/rcab/slider-crank.xml slider-crank-rcab.out

# slider/crank regressions (MCAB)
moby-regress -s=.001 -mt=10 ../example/slider-crank/mcab/slider-crank.xml slider-crank-mcab.out

# articulated body with contacdt regressions
moby-regress -s=.001 -mt=10 ../example/chain-contact/.xml chain-contact.out

